{
    "name": "fabiocicerchia/silex-setup",
    "description": "My Silex App",
    "license": "MIT",
    "require": {
        "php":                                   ">=5.3.3",
        "behat/behat":                           "3.0.*",
        "components/jquery":                     "1.11.*",
        "components/jqueryui":                   "1.10.*",
        "cortext/silex-simpleuser":              "0.6.3",
        "dflydev/doctrine-orm-service-provider": "1.*",
        "doctrine/common":                       "2.4.*",
        "doctrine/dbal":                         "2.4.*",
        "gregoryloucas/fontstrap":               "3.0.2",
        "igorw/config-service-provider":         "1.2.*",
        "knplabs/console-service-provider":      "dev-master",
        "kphoen/negotiation-service-provider":   "~1.0",
        "mheap/Silex-Assetic":                   "1.0.2",
        "monolog/monolog":                       "1.9.0",
        "mrclay/minify":                         "2.2.0",
        "nervo/yuicompressor":                   "2.4.8",
        "phpunit/dbunit":                        "1.3.1",
        "phpunit/php-code-coverage":             "2.0.5",
        "phpunit/php-file-iterator":             "1.3.4",
        "phpunit/php-invoker":                   "1.1.3",
        "phpunit/php-text-template":             "1.2.0",
        "phpunit/php-timer":                     "1.0.*",
        "phpunit/php-token-stream":              "1.2.2",
        "phpunit/phpunit":                       "3.7.*",
        "phpunit/phpunit-mock-objects":          "1.2.3",
        "phpunit/phpunit-selenium":              "1.3.3",
        "phpunit/phpunit-story":                 "1.0.2",
        "silex/silex":                           "1.2.0",
        "silex/web-profiler":                    "~1.0",
        "spiderlabs/owasp-modsecurity-crs":      "2.2.9",
        "symfony/class-loader":                  "2.4.*",
        "symfony/config":                        "2.4.*",
        "symfony/console":                       "2.4.*",
        "symfony/dependency-injection":          "2.4.*",
        "symfony/finder":                        "2.2.*",
        "symfony/form":                          "2.4.*",
        "symfony/monolog-bridge":                "2.4.*",
        "symfony/process":                       "2.4.*",
        "symfony/security":                      "2.4.*",
        "symfony/translation":                   "2.4.*",
        "symfony/twig-bridge":                   "2.4.*",
        "symfony/validator":                     "2.4.*",
        "symfony/yaml":                          "2.4.*",
        "twbs/bootstrap":                        "3.1.1",
        "twig/extensions":                       "1.0.*"
    },
    "require-dev": {
        "facebook/xhprof":               "dev-master",
        "oziks/xhprof-service-provider": "dev-master",
        "symfony/browser-kit":           "2.4.*",
        "symfony/css-selector":          "2.4.*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/facebook/xhprof"
        },
        {
            "type": "package",
            "package": {
                "name": "cortext/silex-simpleuser",
                "version": "0.6.3",
                "source": {
                    "type": "git",
                    "url": "https://github.com/cortext/silex-simpleuser",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "SimpleUser": "src/" }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "gregoryloucas/fontstrap",
                "version": "3.0.2",
                "source": {
                    "type": "git",
                    "url": "https://github.com/gregoryloucas/Fontstrap",
                    "reference": "Font-Awesome-More-v3.0.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "spiderlabs/owasp-modsecurity-crs",
                "version": "2.2.9",
                "source": {
                    "type": "git",
                    "url": "https://github.com/SpiderLabs/owasp-modsecurity-crs",
                    "reference": "2.2.9"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "": "app/",
            "App\\": "src/"
        }
    }
}
