            "type": "git",
            "url": "https://github.com/facebook/xhprof"
        },
        {
            "type": "package",
            "package": {
                "name": "gregoryloucas/fontstrap",
                "version": "3.0.2",
                "source": {
                    "type": "git",
                    "url": "https://github.com/gregoryloucas/Fontstrap",
                    "reference": "Font-Awesome-More-v3.0.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "spiderlabs/owasp-modsecurity-crs",
                "version": "2.2.9",
                "source": {
                    "type": "git",
                    "url": "https://github.com/SpiderLabs/owasp-modsecurity-crs",
                    "reference": "2.2.9"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "": "app/",
            "App\\": "src/"
        }
    }
